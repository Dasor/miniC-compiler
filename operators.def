// Operator definitions for X macro
// Format: OPERATOR(str, enum_name, description)
OPERATOR("+", Plus, "addition operator")
OPERATOR("-", Minus, "subtraction operator")
OPERATOR("*", Star, "multiplication operator")
OPERATOR("/", Slash, "division operator")
OPERATOR("%", Percent, "modulus operator")
OPERATOR("++", PlusPlus, "increment operator")
OPERATOR("--", MinusMinus, "decrement operator")
OPERATOR("->", Arrow, "member access operator")
OPERATOR("&", Amp, "address-of operator")
OPERATOR("|", Bar, "bitwise OR operator")
OPERATOR("^", Caret, "bitwise XOR operator")
OPERATOR("~", Tilde, "bitwise NOT operator")
OPERATOR("!", Exclaim, "logical NOT operator")
OPERATOR("&&", AmpAmp, "logical AND operator")
OPERATOR("||", BarBar, "logical OR operator")
OPERATOR("?", Question, "ternary operator")
OPERATOR(":", Colon, "colon operator")
OPERATOR("=", Assign, "assignment operator")
OPERATOR("+=", PlusEqual, "addition assignment")
OPERATOR("-=", MinusEqual, "subtraction assignment")
OPERATOR("*=", StarEqual, "multiplication assignment")
OPERATOR("/=", SlashEqual, "division assignment")
OPERATOR("%=", PercentEqual, "modulus assignment")
OPERATOR("&=", AmpEqual, "bitwise AND assignment")
OPERATOR("|=", BarEqual, "bitwise OR assignment")
OPERATOR("^=", CaretEqual, "bitwise XOR assignment")
OPERATOR("<", Less, "less than operator")
OPERATOR(">", Greater, "greater than operator")
OPERATOR("<=", LessEqual, "less than or equal")
OPERATOR(">=", GreaterEqual, "greater than or equal")
OPERATOR("==", EqualEqual, "equality operator")
OPERATOR("!=", NotEqual, "inequality operator")
OPERATOR("(", LParen, "left parenthesis")
OPERATOR(")", RParen, "right parenthesis")
OPERATOR("[", LBracket, "left bracket")
OPERATOR("]", RBracket, "right bracket")
OPERATOR("{", LBrace, "left brace")
OPERATOR("}", RBrace, "right brace")
OPERATOR(";", Semicolon, "semicolon")
OPERATOR(",", Comma, "comma")
OPERATOR(".", Dot, "dot operator")
OPERATOR("...", Ellipsis, "ellipsis")
OPERATOR("#", Hash, "hash operator")
OPERATOR("##", HashHash, "token concatenation")
